        <TextBlock x:Name="TimerDisplay" HorizontalAlignment="Left" Margin="10,0,0,0"/>
        <ListBox x:Name="BartenderListBox" HorizontalAlignment="Left" Height="182" Margin="22,56,0,0" VerticalAlignment="Top" Width="215"/>
        <ListBox x:Name="WaitressListBox" HorizontalAlignment="Left" Height="182" Margin="242,56,0,0" VerticalAlignment="Top" Width="215"/>
        <ListBox x:Name="BouncerListBox" HorizontalAlignment="Left" Height="182" Margin="462,56,0,0" VerticalAlignment="Top" Width="215"/>
        <Button x:Name="OpenBarButton" Content="Open Bar" HorizontalAlignment="Left" Margin="22,266,0,0" VerticalAlignment="Top" Width="655" Click="OpeningBar"/>
        <Label x:Name="GuestLabel" Content="Guests" HorizontalAlignment="Left" Margin="682,73,0,0" VerticalAlignment="Top"/>
        <Label x:Name="GlasLable" Content="Glases" HorizontalAlignment="Left" Margin="683,104,0,0" VerticalAlignment="Top"/>
        <Label x:Name="ChairLabel" Content="Chair" HorizontalAlignment="Left" Margin="690,135,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
